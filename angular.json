{
  "version": 1,
  "projects": {
    "imdb-frontend": {
      "projectType": "application",
      "root": "apps/imdb-frontend",
      "sourceRoot": "apps/imdb-frontend/src",
      "prefix": "imdb-frontend",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/imdb-frontend",
            "index": "apps/imdb-frontend/src/index.html",
            "main": "apps/imdb-frontend/src/main.ts",
            "polyfills": "apps/imdb-frontend/src/polyfills.ts",
            "tsConfig": "apps/imdb-frontend/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/imdb-frontend/src/favicon.ico",
              "apps/imdb-frontend/src/assets"
            ],
            "styles": [
              "./node_modules/flag-icons/css/flag-icons.min.css",
              "apps/imdb-frontend/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/imdb-frontend/src/environments/environment.ts",
                  "with": "apps/imdb-frontend/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "imdb-frontend:build:production"
            },
            "development": {
              "browserTarget": "imdb-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "imdb-frontend:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/imdb-frontend/src/**/*.ts",
              "apps/imdb-frontend/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/imdb-frontend"],
          "options": {
            "jestConfig": "apps/imdb-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "imdb-frontend-e2e": {
      "root": "apps/e2e",
      "sourceRoot": "apps/e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/e2e/cypress.json",
            "devServerTarget": "imdb-frontend:serve:development",
            "tsConfig": "apps/e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "imdb-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/imdb-frontend-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "material": {
      "projectType": "library",
      "root": "libs/material",
      "sourceRoot": "libs/material/src",
      "prefix": "imdb-frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/material"],
          "options": {
            "jestConfig": "libs/material/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/material/src/**/*.ts",
              "libs/material/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "imdb-frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "imdb-frontend"
}
